{
  "name": "home",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "remix setup cloudflare",
    "build:vite": "vite build",
    "upload": "pnpm build && run-p upload:*",
    "upload:worker": "wrangler publish counter/worker.ts --name home-counter",
    "upload:site": "wrangler pages publish ./public --project-name 1project-home",
    "build": "NODE_ENV=production run-s build:css build:remix",
    "build:css": "unocss './app/**/*.{ts,tsx}' -m -o ./app/unocss.css",
    "build:remix": "NODE_ENV=production remix build",
    "dev:remix": "NODE_ENV=development remix watch",
    "dev:css": "unocss './app/**/*.{ts,tsx}' -o ./app/unocss.css --watch",
    "dev:wrangler": "NODE_ENV=development wrangler pages dev --live-reload ./public",
    "dev": "unocss './app/**/*.{ts,tsx}' -o ./app/unocss.css && NODE_ENV=development remix build && run-p \"dev:*\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@iconify-json/carbon": "^1.1.7",
    "@remix-run/cloudflare": "^1.6.7",
    "@remix-run/cloudflare-pages": "^1.6.7",
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/server-runtime": "^1.6.7",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@unocss/cli": "^0.45.5",
    "@unocss/preset-icons": "^0.45.5",
    "@unocss/preset-typography": "^0.45.5",
    "@unocss/preset-wind": "^0.45.5",
    "@unocss/reset": "^0.45.5",
    "ditty": "workspace:^1.0.0",
    "eslint": "^8.21.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "remix": "^1.6.7",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.24"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
